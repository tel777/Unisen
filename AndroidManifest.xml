<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.unisen"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <provider android:name="RankProvider" android:authorities="com.example.unisen.rankprovider" />
        <activity
            android:name="com.example.unisen.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.unisen.SubActivity"
            android:label="@string/title_activity_sub" >
        </activity>
        <activity
            android:name="com.example.unisen.CountActivity"
            android:label="@string/title_activity_count" >
        </activity>
        <activity
            android:name="com.example.unisen.RankActivity"
            android:label="@string/title_activity_rank" >
        </activity>
    </application>

</manifest>